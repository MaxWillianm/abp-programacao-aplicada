{
  "name": "abp-programacao-aplicada",
  "version": "1.0.0",
  "description": "ABP - Programação Aplicada",
  "main": "webroot/js/default.js",
  "private": true,
  "scripts": {
    "docker-bash": "docker exec -it shared-apache2-php7 /bin/bash",
    "favicon": "node ./Console/Scripts/favicon.js",
    "watch": "cross-env NODE_ENV=development mix watch",
    "watch-poll": "cross-env NODE_ENV=development mix watch -- --watch-options-poll=1000",
    "hot": "cross-env NODE_ENV=development mix watch --hot",
    "dev": "cross-env NODE_ENV=development mix",
    "prod": "mix --production",
    "start": "npm run watch",
    "build": "npm run prod",
    "upd": "yarn upgrade-interactive --latest"
  },
  "author": "Unisatc",
  "dependencies": {
    "jquery": "^3.7.0",
    "jquery-mask-plugin": "^1.14.16",
    "jquery-validation": "^1.19.5",
    "mustache": "^4.2.0",
    "swiper": "^9.2.4"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.5",
    "autoprefixer": "^10.4.15",
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.3",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^6.0.49",
    "png-to-ico": "^2.1.8",
    "postcss": "^8.4.28",
    "postcss-anywhere-import": "^0.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "sharp": "^0.32.5",
    "tailwindcss": "^3.3.3"
  },
  "browserslist": "> 0.20%, not dead",
  "license": "MIT"
}
